<?xml version="1.0" ?>
<robot  xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:macro name="gazebo_default" params="name color">
  <gazebo reference="${name}">
    <material>${color}</material>
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <self_collide>true</self_collide>
  </gazebo>
</xacro:macro>

</robot>
